!URxvt*termName: rxvt-unicode-256color
URxvt*termName: screen-256color
!URxvt*geom:     130x80
!URxvt*font: xft:Inconsolata:pixelsize=19
!URxvt*font: xft:Terminus:pixelsize=15
!URxvt*boldFont: xft: Terminus:pixelsize=15

URxvt.iso14755: False
URxvt.font: -*-termsyn-medium-*-*-*-11-*-*-*-*-*-*-*
URxvt.boldFont: -*-termsyn-bold-*-*-*-11-*-*-*-*-*-*-*
URxvt.italicFont: -*-termsyn-medium-*-*-*-11-*-*-*-*-*-*-*

URxvt*cursorBlink:	True
URxvt*scrollBar:	False
URxvt.underlineURLs: true
URxvt.matcher.button:   1

URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select
URxvt.url-select.launcher:  chromium
URxvt.url-select.underline: true
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search

